{
    "gpu_ids": [0, 1, 2, 3],
    "output_dir": "./output/test",
    "checkpoint": "",
    "wandb": {
        "project": "DepthFormer",
        "entity": "islab-vision",
        "name": "newcrfs",
        "mode": "online"
    },
    "model": {
        "name": "newcrfs"
    },
    "loss": {
        "alpha": 10.0,
        "beta": 0.15,
        "per_image": false
    },
    "dataset": {
        "data_type": "ONLINE",
        "data_path": "/data/Datasets/KITTI/2012",
        "img_size": [352, 704],
        "use_right": false
    },
    "dataloader": {
        "batch_size": 8,
        "num_workers": 8
    },
    "optimizer": {
        "lr": 0.00002,
        "weight_decay": 0.0
    },
    "scheduler": {
        "name": "onecycle",
        "pct_start": 0.3,
        "div_factor": 25,
        "final_div_factor": 100
    },
    "train": {
        "print_freq": 20,
        "valid_freq": 500,
        "epoch": 25,
        "num_accum": 1,
        "grad_norm": 0.1
    },
    "eval": {
        "max_depth_eval": 88,
        "min_depth_eval": 0.001,
        "garg_crop": true,
        "eigen_crop": false,
        "flip_eval": true
    }
}