{
    "gpu_ids": [0, 1, 2, 3],
    "output_dir": "./output/test",
    "checkpoint": "",
    "wandb": {
        "project": "DepthFormer",
        "entity": "islab-vision",
        "name": "oda2_red_order_swin2_neck_red33",
        "mode": "online"
    },
    "model": {
        "name": "oda2_red_order_swin2",
        "bn_momentum": 0.1,
        "bn_eps": 1.0e-3,
        "encoder_type": "large",
        "dec_dim": 512,
        "num_heads": 8,
        "num_repeats": 3,
        "num_emb": 128,
        "window_size": 8,
        "drop_prob": 0.0,
        "attn_drop_prob": 0.0,
        "bias_type": "depth",
        "neck_type": "red33"
    },
    "loss": {
        "alpha": 10.0,
        "beta": 0.15,
        "per_image": true,
        "si_weight": 1.0,
        "sog_weight": 0.0,
        "reduction_ratio": 8
    },
    "dataset": {
        "data_type": "KITTI",
        "data_path": "/data/Datasets/KITTI/2012",
        "img_size": [352, 704],
        "height_drop": [0.0, 0],
        "width_drop": [0.0, 0],
        "use_right": false
    },
    "dataloader": {
        "batch_size": 8,
        "num_workers": 8
    },
    "optimizer": {
        "lr": 0.0001,
        "betas": [0.9, 0.999],
        "weight_decay": 0.1,
        "eps": 1.0e-6,
        "same_lr": true
    },
    "scheduler": {
        "name": "onecycle",
        "pct_start": 0.25,
        "div_factor": 25,
        "final_div_factor": 100,
        "cycle_momentum": false
    },
    "train": {
        "print_freq": 25,
        "valid_freq": 250,
        "epoch": 24,
        "num_accum": 2,
        "grad_norm": 0.1,
        "freeze_encoder_bn": false,
        "freeze_all_bn": -1
    },
    "eval": {
        "max_depth_eval": 80,
        "min_depth_eval": 0.001,
        "garg_crop": true,
        "eigen_crop": false,
        "flip_eval": false
    }
}